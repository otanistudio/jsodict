{
  "name": "JSODict",
  "version": "1.0.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16":"icon16.png",
    "48":"icon48.png",
    "128": "icon128.png"
  },
  "offline_enabled": true,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}